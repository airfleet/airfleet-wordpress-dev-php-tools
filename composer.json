{
  "name": "airfleet/wordpress-dev",
  "type": "phpcodesniffer-standard",
  "description": "Airfleet development tools for WordPress",
  "authors": [
    {
      "name": "Airfleet",
      "email": "dev@airfleet.co"
    }
  ],
  "require": {
    "php": ">=7.4",
    "squizlabs/php_codesniffer": "3.13.5",
    "dealerdirect/phpcodesniffer-composer-installer": "1.2.0",
    "wp-coding-standards/wpcs": "3.2.0",
    "phpcompatibility/php-compatibility": "^10.0.0@dev",
    "phpcompatibility/phpcompatibility-wp": "^3.0.0@dev",
    "sirbrillig/phpcs-variable-analysis": "2.13.0",
    "phpcsstandards/phpcsutils": "1.2.1",
    "php-parallel-lint/php-parallel-lint": "1.4.0",
    "php-parallel-lint/php-console-highlighter": "1.0.0"
  },
  "extra": {
    "phpcodesniffer-search-depth": 5
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "minimum-stability": "dev"
}
